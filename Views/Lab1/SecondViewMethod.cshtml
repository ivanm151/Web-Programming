@model List<string>

<h2>Sorted fruits list (table)</h2>

<table class="table" style="width:20%">
    <thead>
        <tr>
            <th class="text-center">Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var fruit in Model)
        {
            <tr class="text-center">
                <td>@fruit</td>
            </tr>
        }
    </tbody>
</table>